{
    "cli": {
        "version": ">= 13.2.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": true
            }
        },
        "production": {
            "node": "22.14.0",
            "autoIncrement": true,
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "submit": {
        "production": {
            "ios": {
                "appleId": "YOUR_APPLE_ID_EMAIL",
                "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
                "appleTeamId": "YOUR_APPLE_TEAM_ID"
            }
        }
    }
}